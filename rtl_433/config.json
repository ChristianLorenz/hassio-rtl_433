{
  "name": "RTL_433 to MQTT",
  "version": "0.3",
  "slug": "rtl_433",
  "description": "Decodes 315 & 433MHz RF devices and sends them out via MQTT.  Configure via /config/rtl_433.conf.",
  "url": "https://github.com/ChristianLorenz/hassio-rtl_433",
  "startup": "application",
  "services": ["mqtt:need"],
  "boot": "auto",
  "map": ["config", "ssl"],
  "devices": ["/dev/bus/usb:/dev/bus/usb:rwm"],
  "host_network": "False",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ]
}
